    <!-- http://www.cidoc-crm.org/cidoc-crm/P39_measured -->

    <ObjectProperty rdf:about="&cidoc-crm;P39_measured">
        <rdfs:label xml:lang="de">vermaß</rdfs:label>
        <rdfs:label xml:lang="el">μέτρησε</rdfs:label>
        <rdfs:label xml:lang="en">measured</rdfs:label>
        <rdfs:label xml:lang="fr">a mesuré</rdfs:label>
        <rdfs:label xml:lang="pt">mediu</rdfs:label>
        <rdfs:label xml:lang="ru">измерил</rdfs:label>
        <rdfs:comment xml:lang="en">This property associates an instance of E16 Measurement with the instance of E1 CRM Entity to which it applied. An instance of E1 CRM Entity may be measured more than once. Material and immaterial things and processes may be measured, e.g. the number of words in a text, or the duration of an event.
</rdfs:comment>
        <rdfs:domain rdf:resource="&cidoc-crm;E16_Measurement"/>
